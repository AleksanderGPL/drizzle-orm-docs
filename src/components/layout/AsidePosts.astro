    background-color: #1c1c1c;
  }

  html.dark .aside-posts__wrap:hover {
    background-color: #282828;
  }

  html.dark .aside-posts__wrap:hover .aside-posts__text {
    color: #fff;
  }

  html.dark .aside-posts__wrap:hover .aside-posts__image svg {
    color: #fff;
  }

  .aside-posts__wrap {
    height: 40px;
    width: 100%;
    background-color: #f6f6f7;
    display: flex;
    align-items: center;
    transition: background-color 0.2s ease-in-out;
  }

  .aside-posts__wrap:first-child {
    border-top-left-radius: 4px;
    border-top-right-radius: 4px;
  }

  .aside-posts__wrap:last-child {
    border-bottom-left-radius: 4px;
    border-bottom-right-radius: 4px;
  }

  .aside-posts__wrap:hover {
    background-color: #eff0f3;
  }

  .aside-posts__wrap:hover .aside-posts__arrow-right {
    opacity: 1;
    transform: translateX(0);
  }

  .aside-posts__wrap:hover .aside-posts__text {
    color: #000;
  }

  .aside-posts__wrap:hover .aside-posts__image svg {
    color: #000;
  }

  .aside-posts__image {
    display: flex;
    align-items: center;
    justify-content: center;
    height: 100%;
    aspect-ratio: 1 / 1;
    width: fit-content;
